pipeline{
	agent any
    stages{
	    stage('Build'){
            steps{
		    script{
			    build()
		    }
            }
        }
        stage('Test'){
            steps{
		    script{
			    build()
		    }
            }
        }
        stage('Deploy'){
            steps{
		    script{
			    deploy()
		    }
            }

        }
    }
}
